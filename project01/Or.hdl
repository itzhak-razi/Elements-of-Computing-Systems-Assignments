/**
 * Or gate. out = a or b
 */

CHIP Or {

    IN  a, b;
    OUT out;

    PARTS:
    Not(in=a, out=nota);
    Not(in=b, out=notb);
    Nand(a=nota, b=notb, out=out);

}
